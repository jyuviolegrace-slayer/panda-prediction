{
  "name": "panda-prediction",
  "main": "entrypoint.js",
  "version": "1.0.0",
  "scripts": {
    "dev": "expo start -c",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start -c --web",
    "clean": "rm -rf .expo node_modules",
    "supabase:login": "supabase login",
    "supabase:link:staging": "supabase link --project-ref $SUPABASE_STAGING_PROJECT_ID",
    "supabase:link:prod": "supabase link --project-ref $SUPABASE_PROD_PROJECT_ID",
    "db:push:staging": "supabase link --project-ref $SUPABASE_STAGING_PROJECT_ID && supabase db push",
    "db:push:prod": "supabase link --project-ref $SUPABASE_PROD_PROJECT_ID && supabase db push",
    "types:gen:local": "supabase gen types typescript --local > types/supabase.ts",
    "types:gen:staging": "supabase gen types typescript --project-id $SUPABASE_STAGING_PROJECT_ID > types/supabase.ts",
    "types:gen:prod": "supabase gen types typescript --project-id rhiveaobslchlliqharj > types/supabase.ts"
  },
  "dependencies": {
    "@ethersproject/shims": "^5.8.0",
    "@privy-io/expo": "^0.59.6",
    "@privy-io/expo-native-extensions": "^0.0.5",
    "@react-navigation/native": "^7.0.0",
    "@rn-primitives/label": "^1.2.0",
    "@rn-primitives/portal": "~1.3.0",
    "@rn-primitives/slot": "^1.2.0",
    "@rn-primitives/switch": "^1.2.0",
    "@supabase/supabase-js": "^2.47.12",
    "base64-arraybuffer": "^1.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "expo": "^54.0.0",
    "expo-apple-authentication": "~8.0.7",
    "expo-application": "~7.0.7",
    "expo-crypto": "~15.0.7",
    "expo-file-system": "~19.0.17",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-router": "~6.0.10",
    "expo-secure-store": "~15.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-web-browser": "~15.0.8",
    "fast-text-encoding": "^1.0.6",
    "lucide-react-native": "^0.545.0",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-get-random-values": "^1.11.0",
    "react-native-passkeys": "^0.4.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "^0.21.0",
    "react-native-webview": "13.15.0",
    "react-native-worklets": "0.5.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.14",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@types/react": "~19.1.10",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "typescript": "~5.9.2"
  },
  "private": true
}
